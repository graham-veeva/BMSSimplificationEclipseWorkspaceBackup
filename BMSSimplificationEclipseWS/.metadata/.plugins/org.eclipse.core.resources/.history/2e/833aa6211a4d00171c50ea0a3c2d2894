    <validationRules>
        <fullName>Applicable_Activity_Status_Spec</fullName>
        <active>true</active>
        <description>Applicable Activity Status must be specified if Mandatory or Requires Attachment is checked</description>
        <errorConditionFormula>AND( 
     OR( Requires_Attachment_HCPTS__c , Mandatory_HCPTS__c ) , 
        ISBLANK(TEXT( Applicable_Activity_Status_HCPTS__c )) 
)</errorConditionFormula>
        <errorDisplayField>Applicable_Activity_Status_HCPTS__c</errorDisplayField>
        <errorMessage>Applicable Activity Status cannot be blank for Mandatory Action Items and/or Materials that need Attachment</errorMessage>
