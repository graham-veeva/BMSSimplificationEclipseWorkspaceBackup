<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Global Account Search Settings</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Account_Search_Field_Set__c</fullName>
        <description>List of fields, delimited by string, to use as search fields for the Account.  Sample value:
Name,FirstName,LastName,Specialty_1_vod__c.</description>
        <encrypted>false</encrypted>
        <externalId>false</externalId>
        <label>Account Search Field Set</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Account_Source_Country_fieldname__c</fullName>
        <description>Name of the Account field to use for the Source Country filter field.</description>
        <encrypted>false</encrypted>
        <externalId>false</externalId>
        <label>Account Source Country fieldname</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Address_Search_Field_Set__c</fullName>
        <description>List of fields, delimited by commas, of Address fields to search on. Sample value:
Name,City_vod__c,State_vod__c,Zip_vod__c,Phone_vod__c</description>
        <encrypted>false</encrypted>
        <externalId>false</externalId>
        <label>Address Search Field Set</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>ENABLE_Source_Country_Filter__c</fullName>
        <defaultValue>false</defaultValue>
        <description>When Checked enables filtering by source country.</description>
        <externalId>false</externalId>
        <label>ENABLE_Source_Country_Filter</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Result_Field_Set__c</fullName>
        <description>List of fields, delimited by comma, to display in search results.  If any of these are Address fields, they must be prefixed by &apos;ADD.&apos;.  Sample value:
Name,ADD.Name,ADD.City_vod__c,ADD.State_vod__c,ADD.Zip_vod__c,Specialty_1_vod__c,ADD.Primary_vod__c</description>
        <encrypted>false</encrypted>
        <externalId>false</externalId>
        <label>Result Field Set</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Show_All_Addresses__c</fullName>
        <defaultValue>false</defaultValue>
        <description>If checked, display all addresses for an Account; otherwise, only display primary address. If no primary address, display the first one found.</description>
        <externalId>false</externalId>
        <label>Show_All_Addresses</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>User_Source_Country_fieldname__c</fullName>
        <description>Name of the field on the User object for the Source Country filter.</description>
        <encrypted>false</encrypted>
        <externalId>false</externalId>
        <label>User Source Country fieldname</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>GASSettings</label>
    <visibility>Public</visibility>
</CustomObject>
