    <validationRules>
        <fullName>Controlled_Sub_Schedule_Confirm_vod</fullName>
        <active>true</active>
        <description>This validation rules is in place to confirm that a Product, which has been marked as a Controlled Substance has a Schedule.</description>
        <errorConditionFormula>AND( Controlled_Substance_vod__c ,TRIM( Schedule_vod__c) = &quot;&quot;)</errorConditionFormula>
        <errorMessage>The Schedule field is required when creating a Controlled Substance Product.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Product_Bundle_Check_vod</fullName>
        <active>true</active>
        <description>Verifies that the Product Type is a Product Group if the Bundle Pack field is enabled</description>
        <errorConditionFormula>Bundle_Pack_vod__c &amp;&amp; !ISPICKVAL(Product_Type_vod__c, &apos;Product Group&apos;)</errorConditionFormula>
        <errorMessage>Bundle_Pack_vod__c can be set to TRUE only if the product type= Product Group</errorMessage>
    </validationRules>
