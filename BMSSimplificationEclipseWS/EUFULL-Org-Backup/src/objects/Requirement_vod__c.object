        <fullName>BMS_EMEA_CR_not_closed_without_Release</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
 RecordType.DeveloperName = &apos;BMS_EMEA_CORE_Change_Request&apos;,
ISPICKVAL(Status_PMO__c, &apos;Release: SIT&apos;),
 Release__c = &apos;&apos;
)</errorConditionFormula>
        <errorMessage>The Change Request cannot be closed without a targeted Release date. Please confirm the Release date with the Team first.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Positive_Effort</fullName>
        <active>true</active>
        <description>All numbers entered under level of effort should be positive.</description>
        <errorConditionFormula>OR( Adapt__c &lt; 0,  BUT__c &lt; 0, Des__c &lt; 0, SIT__c &lt; 0, TrnDep__c &lt; 0, UAT__c &lt;0, Req__c &lt;0)</errorConditionFormula>
        <errorMessage>Please enter a value greater than 0 for fields under Total Level of Effort.</errorMessage>
    </validationRules>
