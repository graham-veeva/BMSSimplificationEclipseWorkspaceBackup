    <fields>
        <fullName>Generate_Invoice_Validation_HCPTS__c</fullName>
        <encrypted>false</encrypted>
        <externalId>false</externalId>
        <formula>If(
  OR(
     ISBLANK(Purchase_Order_Number__c),
     ISBLANK(Vendor_ID_2_HCPTS__c),
     AND(
       NOT(ISPICKVAL(Status_vod__c ,&quot;PO Created&quot;)),
       NOT(ISPICKVAL(Status_vod__c ,&quot;PO Updated&quot;))
     )
  )
  ,
  &quot;Requirements not met&quot;
  ,
  &quot;Requirements met&quot;
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Generate Invoice Validation</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Generate_Invoice__c</fullName>
        <description>+ IF( ISPICKVAL( Document_Type_HCPTS__c , &quot;Invoice for Fees - Fixed&quot;) , &quot;a4zZ00000000SZE,a4zZ00000000SfI&quot;, &quot;a4zZ00000000S9q&quot;)</description>
        <encrypted>false</encrypted>
        <externalId>false</externalId>
        <formula>HYPERLINK( 
&quot;https://composer.congamerge.com&quot; + 
&quot;?sessionId=&quot; +  $Api.Session_ID +
&quot;&amp;serverUrl=&quot; +  $Api.Partner_Server_URL_290 + 
&quot;&amp;id=&quot; + Id +
&quot;&amp;CurrencyCulture=de-DE&quot; + 

&quot;&amp;TemplateGroup=&quot; + Conga_Invoice_Group__c +
&quot;&amp;CongaEmailTemplateGroup=none&quot; +
&quot;&amp;SelectTemplates=2&quot; + 
&quot;&amp;DefaultPDF=1&quot; + 
&quot;&amp;DC=&quot; + IF(Generate_Invoice_Validation_HCPTS__c = &quot;Requirements met&quot;,&quot;0&quot;,&quot;1&quot;) +
&quot;&amp;DCL=&quot; + $Label.VPROF_Invoice_Error +
&quot;&amp;DS3=1&quot; +

&quot;&amp;QueryId=[Event]&quot; + $Setup.Conga_and_Echosign_Ids__c.Expense_Invoice_Event__c + &quot;?pv0=&quot; +   Event_vod__r.Id  +
&quot;,[Speaker]&quot; + $Setup.Conga_and_Echosign_Ids__c.Expense_Invoice_Speaker__c + &quot;?pv0=&quot; +  Incurred_Expense_Speaker_vod__r.Id +
&quot;,[Fees]&quot; + $Setup.Conga_and_Echosign_Ids__c.Expense_Invoice_Fees__c + &quot;?pv0=&quot; +  Incurred_Expense_Speaker_vod__r.Event_Speaker_Contract_HCPTS__r.Id + &quot;,[ContractParty]&quot; + $Setup.Conga_and_Echosign_Ids__c.Expense_Invoice_Contract_Party__c + &quot;?pv0=&quot; +  Payee_Vendor_vod__r.Id  + &quot;,[ExpLines]&quot; + $Setup.Conga_and_Echosign_Ids__c.Expense_Invoice_Exp_Lines__c + &quot;?pv0=&quot; +  Id  + &quot;,[ActAgreement]&quot; +  $Setup.Conga_and_Echosign_Ids__c.Expense_Invoice_Act_Agreement__c  + &quot;?pv0=&quot; + Id +
&quot;&amp;SC0=1&quot; + 
&quot;&amp;AC0=1&quot; + 
&quot;&amp;SC1=Attachments&quot; + 
&quot;&amp;DS7Preview=1&quot; +
&quot;&amp;DS7=0&quot; +
&quot;&amp;OFN=Activity+Invoice+for+&quot;  +
SUBSTITUTE(
  SUBSTITUTE(
       SUBSTITUTE(
           SUBSTITUTE(
               SUBSTITUTE(
                   SUBSTITUTE(
                       SUBSTITUTE(
                           SUBSTITUTE(
                               SUBSTITUTE(
                                   Payee_Vendor_vod__r.Name,
                                   &quot;#&quot;,
                                   &quot;&quot;),
                               &quot;$&quot;,
                               &quot;&quot;),
                           &quot;%&quot;,
                           &quot;&quot;),
                       &quot;&amp;&quot;,
                       &quot;&quot;),
                   &quot;\\&quot;,
                   &quot;&quot;),
               &quot;?&quot;,
               &quot;&quot;),
           &quot;/&quot;,
           &quot;&quot;),
       &quot;!&quot;,
       &quot;&quot;),
    &quot;*&quot;,
&quot;&quot;)

, &quot;Generate Invoice&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Generate Invoice</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
