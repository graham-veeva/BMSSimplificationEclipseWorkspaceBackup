        <encrypted>false</encrypted>
        <externalId>false</externalId>
        <formula>IF(LEN( Account_vod__c) &gt; 0, &quot;Account&quot;,  IF(LEN( Contact_vod__c) &gt; 0, &quot;Contact&quot;,  &quot;User&quot;) )</formula>
