    <validationRules>
        <fullName>Attendee_Selection_vod</fullName>
        <active>true</active>
        <description>This rule validates that either an Account, Contact, or User was selected as an Attendee.</description>
        <errorConditionFormula>AND(LEN( Account_vod__c ) &lt; 1,LEN( Contact_vod__c ) &lt; 1,LEN( User_vod__c ) &lt; 1,LEN(EM_Attendee_vod__c) &lt; 1,LEN(EM_Event_Speaker_vod__c) &lt; 1, ISBLANK(TEXT(Walk_In_Status_vod__c)))</errorConditionFormula>
        <errorMessage>An Account, Contact, or User must be selected to save a record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMS_EMEA_FFM_Attendee_Co_Author</fullName>
        <active>true</active>
        <description>VPRO 27 Feb 2014: Validation to not allow a co-author of the meeting to modify any fields.</description>
        <errorConditionFormula>AND( 
ISBLANK( EM_Event_Speaker_vod__c ),
ISBLANK( EM_Attendee_vod__c ),
 
$User.Id &lt;&gt;  CreatedById,
TEXT( Medical_Event_vod__r.FFM_Step_BMS_EMEA__c ) = &apos;Meeting Post-Execution&apos;,
OR(
$Profile.Name = &apos;BMS - EMEA - REP - MSL&apos;,
$Profile.Name = &apos;BMS - EMEA - REP - SALES&apos;),
OR( 
ISCHANGED(Status_vod__c	) = True,
ISCHANGED(Account_vod__c) = True)
)</errorConditionFormula>
        <errorMessage>You are not allowed to modify any fields of a Meeting Attendee added by another user.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Multiple_Attendees_vod</fullName>
        <active>true</active>
        <description>This validation Rule will confirm that only one Account or Contact or User is selected as the Attendee.</description>
        <errorConditionFormula>IF( LEN( Account_vod__c ) &gt; 0 , IF( OR(LEN( Contact_vod__c ) &gt; 0 ,LEN( User_vod__c ) &gt; 0),true,false) , IF( LEN( Contact_vod__c ) &gt; 0 , IF( OR(LEN( Account_vod__c ) &gt; 0 ,LEN( User_vod__c ) &gt; 0),true,false) , IF( LEN( User_vod__c ) &gt; 0 , IF( OR(LEN( Account_vod__c ) &gt; 0 ,LEN( Contact_vod__c ) &gt; 0),true,false) , false)))</errorConditionFormula>
        <errorMessage>Please select only one Account, Contact, or User.</errorMessage>
    </validationRules>
