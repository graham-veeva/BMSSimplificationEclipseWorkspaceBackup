    <validationRules>
        <fullName>VEEVA_DATES_MISMATCH_CHECK</fullName>
        <active>true</active>
        <description>This validation rule ensures that the Start Date always falls before the End Date.</description>
        <errorConditionFormula>Allocation_Start_Date_vod__c &gt; Allocation_End_Date_vod__c || Order_Start_Date_vod__c &gt; Order_End_Date_vod__c</errorConditionFormula>
        <errorMessage>Start date cannot be after end date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VEEVA_MINIMUM_ORDER_CHECK</fullName>
        <active>true</active>
        <description>This rule enforces that the minimum order is not greater than the allocation quantity.</description>
        <errorConditionFormula>Minimum_Single_Order_Quantity_vod__c &gt; Product_Order_Allocation_Quantity_vod__c</errorConditionFormula>
        <errorMessage>Minimum order quantity cannot be greater than the allocation quantity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VEEVA_ORDER_DATES_CHECK</fullName>
        <active>true</active>
        <description>This validation rule enforces Order Dates falling within the Allocation Dates.</description>
        <errorConditionFormula>Order_Start_Date_vod__c &lt; Allocation_Start_Date_vod__c || Order_End_Date_vod__c &gt; Allocation_End_Date_vod__c</errorConditionFormula>
        <errorMessage>Order Dates must fall within Allocation Dates</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VEEVA_QUANTITIES_CHECK</fullName>
        <active>true</active>
        <description>This rule enforces that the allocation quantity or minimum order quantity are positive numbers.</description>
        <errorConditionFormula>Product_Order_Allocation_Quantity_vod__c &lt; 0 || Minimum_Single_Order_Quantity_vod__c &lt; 0</errorConditionFormula>
        <errorMessage>Quantity cannot be less than 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VEEVA_QUANTITY_FULFILLED_CHECK</fullName>
        <active>true</active>
        <description>This validation rule ensures that the allocation quantity cannot be less than the fulfilled quantity.</description>
        <errorConditionFormula>ISPICKVAL(Allow_Quantity_Exceptions_vod__c, &apos;Prevent_vod&apos;)  &amp;&amp; Product_Order_Allocation_Quantity_vod__c &lt; Total_Order_Fulfilled_Quantity_vod__c</errorConditionFormula>
        <errorMessage>Allocation quantity cannot be less than the fulfilled quantity.</errorMessage>
    </validationRules>
</CustomObject>
