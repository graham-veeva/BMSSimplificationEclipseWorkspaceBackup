    <fields>
        <fullName>DCR_Record_Type_BMS_EMEA__c</fullName>
        <description>BMS ACN - June 2016: Field to display the DCR Record Type to be used in Email Notifications.. Created as part of the DCR OOB Project.</description>
        <encrypted>false</encrypted>
        <externalId>false</externalId>
        <formula>IF( AND(RecordType.Name = &apos;Account_vod&apos; ,TEXT(Type_vod__c) = &apos;Edit_vod&apos;), &apos;Customer EDIT​&apos;&amp;&apos; : &apos;&amp;Account_vod__r.Name&amp;&quot; ​&quot;&amp;Account_vod__r.FirstName&amp;&quot; ​&quot;&amp;Account_vod__r.LastName, 
IF( AND(RecordType.Name = &apos;Account_vod&apos; ,TEXT(Type_vod__c) = &apos;New_vod&apos;), &apos;Customer ADD&apos;,
IF( AND(RecordType.Name = &apos;Address_vod&apos; ,TEXT(Type_vod__c) = &apos;Edit_vod&apos;), &apos;Address EDIT&apos;&amp;&apos; : &apos;&amp;Address_vod__r.Name&amp;&quot; for Customer: ​&quot;&amp;Account_vod__r.Name&amp;&quot; ​&quot;&amp;Account_vod__r.FirstName&amp;&quot; ​&quot;&amp;Account_vod__r.LastName, 
IF( AND(RecordType.Name = &apos;Address_vod&apos; ,TEXT(Type_vod__c) = &apos;New_vod&apos;), &apos;Address ADD&apos;,
IF( AND(RecordType.Name = &apos;Child_Account_vod&apos; ,TEXT(Type_vod__c) = &apos;Edit_vod&apos;), &apos;Child Account EDIT&apos;&amp;&apos; : &apos;&amp;Child_Account_vod__r.Parent_Child_Name_vod__c , 
IF( AND(RecordType.Name = &apos;Child_Account_vod&apos; ,TEXT(Type_vod__c) = &apos;New_vod&apos;), &apos;Child Account ADD&apos;, &apos;&apos;))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>DCR Record Type</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
