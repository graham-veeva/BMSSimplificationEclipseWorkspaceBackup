    <validationRules>
        <fullName>Banking_Info_Required_to_Sign_Manually</fullName>
        <active>false</active>
        <description>If Decline OOP is not checked, the following banking fields are required to sign manually: Name, Address, City, District, State/Province, Postal Code, Country, Number (4 digits), Branch Number (4 digits), Account Number, IBAN, CPF/CNPJ.</description>
        <errorConditionFormula>AND(
  ISPICKVAL(Status_HCPTS__c, &quot;Signed Manually&quot;) 
  ,
  Decline_OOP_HCPTS__c = false
  ,
  OR(
    ISBLANK(Bank_Name_HCPTS__c),
    ISBLANK(Bank_Address_HCPTS__c),
    ISBLANK(Bank_City_HCPTS__c),
    ISBLANK(Bank_District_HCPTS__c),
    ISBLANK(Bank_State_Province_HCPTS__c),
    ISBLANK(Bank_Postal_Code_HCPTS__c),
    ISBLANK(Bank_Country_HCPTS__c),
    ISBLANK(Account_Number_HCPTS__c),
    OR(
      ISBLANK(Bank_Number_HCPTS__c),
      LEN(Bank_Number_HCPTS__c)&lt;&gt;4
    ),
    OR(
      ISBLANK(Bank_Branch_Number_HCPTS__c),
      LEN(Bank_Branch_Number_HCPTS__c)&lt;&gt;4
    ),
    ISBLANK(IBAN_Number_HCPTS__c),
    ISBLANK(Contract_Tax_ID_HCPTS__c)
  )
)</errorConditionFormula>
        <errorMessage>If Decline OOP is not checked, the following banking fields are required to sign manually: Name, Address, City, District, State/Province, Postal Code, Country, Number (4 digits), Branch Number (4 digits), Account Number, IBAN, CPF/CNPJ.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ICF_Required_to_Submit_for_Signature</fullName>
        <active>true</active>
        <description>The following is required to submit an ICF for Signature:
Speaker, Speaker&apos;s Country, Speaker&apos;s Preferred Language and Source Activity</description>
        <errorConditionFormula>AND
(
ISPICKVAL(Status_HCPTS__c, &apos;Out for Signature&apos;),
ISBLANK(Speaker_HCPTS__c),
ISBLANK(Activity_Source_2__c),
ISBLANK(Country_Formula_HCPTS__c),
ISBLANK(Preferred_Language_HCPTS__c)
)</errorConditionFormula>
        <errorMessage>The following is required to submit an ICF for Signature:
Speaker, Speaker&apos;s Country, Speaker&apos;s Preferred Language and Source Activity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_to_Sign_Manually_Events</fullName>
        <active>true</active>
        <description>The Following Is Require to Sign: Contract Party Country, Signature, Signer&apos;s Name, Signer&apos;s Title, Signer&apos;s Email, Phone, Address Line 1, City, Postal Code</description>
        <errorConditionFormula>AND(
RecordType.DeveloperName = &apos;ICF_Events&apos;,
ISPICKVAL(Status_HCPTS__c, &quot;Signed Manually&quot;),
ISBLANK(Signer_Name_HCPTS__c) ||
ISBLANK(Signer_Title_HCPTS__c) ||
ISBLANK(Signer_Email_HCPTS__c) ||
ISBLANK ( Signature_Date_HCPTS__c )  || 
ISBLANK( Address_Line_1_HCPTS__c ) || 
ISBLANK( City_HCPTS__c ) || 
ISBLANK( Postal_Code_HCPTS__c ) || 
ISBLANK( Phone_Number_HCPTS__c ) ||
ISBLANK( Country_vod__c )

)</errorConditionFormula>
