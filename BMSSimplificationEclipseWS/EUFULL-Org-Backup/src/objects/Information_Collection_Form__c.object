    <fields>
        <fullName>ICF_End_Date_HCPTS__c</fullName>
        <description>Date of ICF Expiration Calculated for 2 years after the Signature Date. Formula to acknowlege leap year.</description>
        <encrypted>false</encrypted>
        <externalId>false</externalId>
        <formula>IF(
  AND(
    MONTH( Signature_Date_HCPTS__c ) = 2,
    DAY( Signature_Date_HCPTS__c ) = 29,
    NOT( 
      OR( 
        MOD( YEAR( Signature_Date_HCPTS__c ), 400 ) = 0, 
        AND( 
          MOD( YEAR( Signature_Date_HCPTS__c ), 4 ) = 0,
          MOD( YEAR( Signature_Date_HCPTS__c ), 100 ) != 0
        )
      )
    ) 
  ),
  DATE( YEAR( Signature_Date_HCPTS__c ) + 2, 3, 1),
  DATE( YEAR( Signature_Date_HCPTS__c ) + 2, MONTH( Signature_Date_HCPTS__c ), 
  DAY( Signature_Date_HCPTS__c ) )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>ICF Expiry Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
