    <validationRules>
        <fullName>BMS_EMEA_FFM_DoS_Check</fullName>
        <active>true</active>
        <description>BMS EMEA - Feb Mini LRM 2017 : Validation Rule to check if sum of 6 breakdown fields is equal to Estimated Duration of Service.</description>
        <errorConditionFormula>IF(AND(OR 
(ISBLANK(Estimated_Duration_of_Service_hrs__c), Estimated_Duration_of_Service_hrs__c &lt; 0),
OR(
ISPICKVAL($User.User_Source_Country_BMS_CORE__c , &apos;GB&apos;),
ISPICKVAL($User.User_Source_Country_BMS_CORE__c , &apos;IE&apos;),
ISPICKVAL($User.User_Source_Country_BMS_CORE__c , &apos;UI&apos;))), TRUE,  
AND(OR(
ISPICKVAL($User.User_Source_Country_BMS_CORE__c , &apos;GB&apos;),
ISPICKVAL($User.User_Source_Country_BMS_CORE__c , &apos;IE&apos;),
ISPICKVAL($User.User_Source_Country_BMS_CORE__c , &apos;UI&apos;)),
(Estimated_Duration_of_Service_hrs__c  &lt;&gt; 
ROUND(((Briefing_mins_BMS_EMEA__c + Presentation_mins_BMS_EMEA__c + Chairing_mins_BMS_EMEA__c + Q_and_A_mins_BMS_EMEA__c + Post_Meeting_Debrief_mins_BMS_EMEA__c + Other_mins_BMS_EMEA__c)/60),2))))</errorConditionFormula>
        <errorDisplayField>Estimated_Duration_of_Service_hrs__c</errorDisplayField>
        <errorMessage>Estimated Duration of Service should be equal to SUM of 6 Breakdown Fields.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMS_EMEA_FFM_Speaker_Co_Author</fullName>
        <active>true</active>
        <description>VPRO 7 March 2014: Validation to not allow a co-author of the meeting to modify any fields.</description>
        <errorConditionFormula>AND(
$User.Id &lt;&gt;   Meeting__r.OwnerId,
TEXT( Meeting__r.FFM_Step_BMS_EMEA__c ) = &apos;Meeting Post-Execution&apos;,
OR( 
$Profile.Name = &apos;BMS - EMEA - REP - MSL&apos;, 
$Profile.Name = &apos;BMS - EMEA - REP - SALES&apos;) 
)</errorConditionFormula>
        <errorMessage>You are not allowed to change any fields as you are co-author of the Meeting.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMS_EMEA_FFM_Speaker_CreateWhileApproval</fullName>
        <active>true</active>
        <description>BMS EMEA Aug 2014: Prevents users from creating a Speaker while the Meeting is pending approval</description>
        <errorConditionFormula>AND( 
ISNEW(), 
ISPICKVAL(Meeting__r.Status_BMS_CORE__c, &apos;Submitted - Waiting Approval&apos;), 
TEXT(Meeting__r.FFM_Step_BMS_EMEA__c ) &lt;&gt; &apos;&apos; 
)</errorConditionFormula>
        <errorMessage>You can add Speaker while the Meeting is waiting for approval. Please cancel and go back to the Meeting page.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMS_EMEA_Meeting_Speaker_Comments_Check</fullName>
        <active>true</active>
        <description>BMS ACN -  September : Q3 GRM 2016 - Prevent users to save a Meeting Speaker which was not delivered without comments.</description>
        <errorConditionFormula>AND (ISPICKVAL(Delivered_BMS_EMEA__c, &quot;No&quot;),  ISBLANK(Comments_BMS_IC__c))</errorConditionFormula>
        <errorDisplayField>Comments_BMS_IC__c</errorDisplayField>
        <errorMessage>Please enter a reason for not delivering the service!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMS_EMEA_Meeting_Speaker_Edit_On_Cancel</fullName>
        <active>true</active>
        <description>BMS ACN - June 2015 - Prevent users from editing meeting speakers records upon cancellation of meeting.</description>
        <errorConditionFormula>Meeting__r.RecordType.DeveloperName == &apos;BMS_EMEA_4_Meeting_Cancelled&apos;</errorConditionFormula>
        <errorMessage>You cannot modify the Speaker of a Cancelled meeting.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMS_EMEA_Meeting_Speaker_Org_Type</fullName>
        <active>true</active>
        <description>BMS ACN - Feb 2016 - Prevent user to create master file if account record type is BMS Organisation.</description>
        <errorConditionFormula>NOT(Account__r.RecordType.DeveloperName=&apos;BMS_Individual&apos;)</errorConditionFormula>
        <errorMessage>Account record type cannot be &quot;Organisation&quot; Type.</errorMessage>
    </validationRules>
