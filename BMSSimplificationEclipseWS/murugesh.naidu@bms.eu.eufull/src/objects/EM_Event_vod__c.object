    <fields>
        <fullName>Send_Activity_Details_Report_Email__c</fullName>
        <description>Link to send email containing Conga Report for Activity Details</description>
        <encrypted>false</encrypted>
        <externalId>false</externalId>
        <formula>HYPERLINK( 

&quot;https://composer.congamerge.com/composer8/index.html?SessionId=&quot; 
+ $Api.Session_ID + &quot;&amp;ServerURL=&quot; + $Api.Partner_Server_URL_290 + 
&quot;&amp;id=&quot; + Id + 
&quot;&amp;QueryID=[ServicesList]&quot; + $Setup.Conga_and_Echosign_Ids__c.ContractAndRelationsForServices__c + &quot;?pv0=&quot; + Id + &quot;,&quot; + 
&quot;[SponsorshipList]&quot; + $Setup.Conga_and_Echosign_Ids__c.ContractAndRelationsForSponsorship__c + &quot;?pv0=&quot; + Id + &quot;,&quot; + 
&quot;[EventsList]&quot; + $Setup.Conga_and_Echosign_Ids__c.ContractAndRelationsForEvents__c + &quot;?pv0=&quot; + Id + &quot;,&quot; + 
&quot;[Activity]&quot; + $Setup.Conga_and_Echosign_Ids__c.ActivityDetails__c + &quot;?pv0=&quot; + Id + &quot;,&quot; + 
&quot;[TeamMembers]&quot; + $Setup.Conga_and_Echosign_Ids__c.Activity_Details_Team_Members__c + &quot;?pv0=&quot; + Id + &quot;,&quot; + 
&quot;[Materials]&quot; + $Setup.Conga_and_Echosign_Ids__c.Materials_for_Event__c + &quot;?pv0=&quot; + Id + &quot;,&quot; + 
&quot;[ExpEstimates]&quot; + $Setup.Conga_and_Echosign_Ids__c.Activity_Details_Exp_Estimates__c + &quot;?pv0=&quot; + Id + &quot;,&quot; + 
&quot;[Audience]&quot; + $Setup.Conga_and_Echosign_Ids__c.Activity_Details_Audience__c + &quot;?pv0=&quot; + Id + &quot;,&quot; + 
&quot;[Participants]&quot; + $Setup.Conga_and_Echosign_Ids__c.ActivityDetailsParticipants__c + &quot;?pv0=&quot; + Id +
&quot;&amp;CongaEmailTemplateId=&quot; + $Setup.Conga_and_Echosign_Ids__c.Activity_Details_Template__c + 
&quot;&amp;EmailSubject=Activity+Details&quot; + 
&quot;&amp;TemplateID=&quot; + $Setup.Conga_and_Echosign_Ids__c.Activity_Details__c + 
&quot;&amp;DS7=2&quot; + 
&quot;&amp;DefaultPDF=1&quot; 
, Activity_ID_2_HCPTS__c ,&quot;_blank&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Send Event Details Report Email</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
