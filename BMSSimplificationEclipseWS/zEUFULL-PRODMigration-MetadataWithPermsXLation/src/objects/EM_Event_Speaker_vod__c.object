 ,
 0, 1) ,
 
&apos;Yes&apos;, 

IF
(
  AND( 
       ISPICKVAL(Service_Type_HCPTS__c,&apos;Services&apos;),
       ISPICKVAL(Event_Speaker_Contract_HCPTS__r.Status_vod__c,&apos;Activated_vod&apos;),
       ISPICKVAL( Contract_Format_HCPTS__c ,&apos;Short-term&apos;)
    )
 ,
 0, 1) ,
 
&apos;Yes&apos;, 


IF
(
  AND( 
       ISPICKVAL(Service_Type_HCPTS__c,&apos;Sponsorship&apos;),
       ISPICKVAL(Event_Speaker_Contract_HCPTS__r.Status_vod__c,&apos;Activated_vod&apos;)
     )
 ,
 0, 1) ,
 
&apos;Yes&apos;, 

IF
(
  AND( 
       ISPICKVAL(Service_Type_HCPTS__c,&apos;Events&apos;),
       ISPICKVAL(Event_Speaker_Contract_HCPTS__r.Status_vod__c,&apos;Activated_vod&apos;)
     )
 ,
 0, 1) ,
 
&apos;Yes&apos;,


&apos;No&apos;)</formula>
