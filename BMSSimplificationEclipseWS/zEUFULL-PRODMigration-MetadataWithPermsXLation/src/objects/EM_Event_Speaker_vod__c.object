    <validationRules>
        <fullName>Cannot_Edit_after_Activity_Approved</fullName>
        <active>false</active>
        <description>Cannot Edit the Activity Speaker Details after the Activity has been Approved</description>
        <errorConditionFormula>ISPICKVAL(Event_vod__r.Status_vod__c, &apos;In Planning&apos;)  &amp;&amp; 
(ISCHANGED(Service_Type_HCPTS__c)  || 
ISCHANGED(Service_Subtype_HCPTS__c)  || 
ISCHANGED(Related_ICF_HCPTS__c) ||
ISCHANGED(Speaker_Contract_HCPTS__c) ||
ISCHANGED(Event_Speaker_Contract_HCPTS__c)|| ISCHANGED(Employer_Name_HCPTS__c) ||
ISCHANGED(Employer_Email_HCPTS__c) ||
ISCHANGED(Employer_Signatory_Name_HCPTS__c) ||
ISCHANGED(Employer_Signatory_Title_HCPTS__c)
)</errorConditionFormula>
        <errorMessage>Cannot edit an Activity Speaker after Activity has been approved</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_have_more_than_10_Signers</fullName>
        <active>true</active>
        <description>There cannot be more than 10 signers.</description>
        <errorConditionFormula>Number_of_Signers_HCPTS__c &gt; 10</errorConditionFormula>
        <errorMessage>There cannot be more than 10 signers.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContractGen_AA_Already_Active</fullName>
        <active>true</active>
        <description>Validation rule that the Subtype cannot be blank when trying to generate a contract</description>
        <errorConditionFormula>AND(
Can_Generate_Contract__c = &apos;true&apos;,
 TEXT(Event_Speaker_Contract_HCPTS__r.Status_vod__c) = &apos;Activated_vod&apos; 
)</errorConditionFormula>
        <errorDisplayField>Event_Speaker_Contract_HCPTS__c</errorDisplayField>
        <errorMessage>Activity Agreement is already activated! You cannot generate and send this Contract again to the HCP!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContractGen_ES_Status_Activated</fullName>
        <active>true</active>
        <description>Validation rule for contract generation: cannot generate contract if the event speaker contract status is activated</description>
        <errorConditionFormula>AND(
Can_Generate_Contract__c = &apos;true&apos;,
TEXT(Event_Speaker_Contract_HCPTS__r.Status_vod__c) = &apos;Activated_vod&apos;
)</errorConditionFormula>
        <errorMessage>Activity Agreement must not be already Activated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContractGen_End_Date</fullName>
        <active>true</active>
        <description>Validation rule for contract generation: Start Date of SOW cannot be after End date of Framework Agreement</description>
        <errorConditionFormula>AND(
Can_Generate_Contract__c = &apos;true&apos;,
ISPICKVAL(Service_Type_HCPTS__c,&apos;Services&apos;),
Event_Speaker_Contract_HCPTS__r.Start_Date_vod__c &gt; Speaker_Contract_HCPTS__r.End_Date_vod__c
)</errorConditionFormula>
        <errorMessage>Start Date of SOW cannot be after End date of Framework Agreement</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContractGen_Event_In_Planning</fullName>
        <active>true</active>
        <description>Validation rule for contract generation: Event must be in In Planning status</description>
        <errorConditionFormula>AND(
Can_Generate_Contract__c = &apos;true&apos;,
TEXT( Event_vod__r.Status_vod__c ) &lt;&gt; &apos;In Planning&apos;
)</errorConditionFormula>
        <errorMessage>Event must be in In Planning status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContractGen_Format_Must_For_Services</fullName>
        <active>true</active>
        <description>Validation rule for contract generation: Services COntract must have Format specified</description>
        <errorConditionFormula>AND(
Can_Generate_Contract__c = &apos;true&apos;,
ISPICKVAL(Service_Type_HCPTS__c,&apos;Services&apos;),
ISBLANK( TEXT(Contract_Format_HCPTS__c))
)</errorConditionFormula>
        <errorMessage>Contract Format must be specified as Long-term or Short-term. Please make a selection. Long-term contracts generate a Framework and Activity Agreement. Short-term contracts only contain an Activity agreement.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContractGen_Framework_Agreement</fullName>
        <active>true</active>
        <description>Validation rule for contract generation: a framework agreement is required if the contract type is &apos;Services&apos;</description>
        <errorConditionFormula>AND(
Can_Generate_Contract__c = &apos;true&apos;,
ISPICKVAL(Service_Type_HCPTS__c,&apos;Services&apos;),
ISBLANK( Speaker_Contract_HCPTS__c)
)</errorConditionFormula>
        <errorMessage>A Framework Agreement is required for a Service Contract</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContractGen_Language_Must</fullName>
        <active>true</active>
        <description>Validation rule for contract generation: All Contracts must specify the Language for a possible Contract template selection</description>
        <errorConditionFormula>AND(
Can_Generate_Contract__c = &apos;true&apos;,
ISBLANK( TEXT(Contract_Language_HCPTS__c))
)</errorConditionFormula>
        <errorMessage>Contract Language value must be specified. Please pick a value!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContractGen_Need_Activity_Agreement</fullName>
        <active>true</active>
        <description>Validation rule for contract generation: Related Activity agreement must be populated</description>
        <errorConditionFormula>AND(
Can_Generate_Contract__c = &apos;true&apos;,
ISBLANK( Event_Speaker_Contract_HCPTS__c)
)</errorConditionFormula>
        <errorMessage>Related Activity agreement must be populated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContractGen_Need_Signers</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
Can_Generate_Contract__c = &apos;true&apos;, 
Number_of_Signers_HCPTS__c &lt; 1 
)</errorConditionFormula>
        <errorMessage>There must be at least one signer to generate a contract.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContractGen_Related_ICF_Signed</fullName>
        <active>true</active>
        <description>Validation rule for contract generation: the related ICF must be signed or signed manually</description>
        <errorConditionFormula>AND(
Can_Generate_Contract__c = &apos;true&apos;,
TEXT(Related_ICF_HCPTS__r.Status_HCPTS__c) &lt;&gt; &apos;Signed&apos;,
TEXT(Related_ICF_HCPTS__r.Status_HCPTS__c) &lt;&gt; &apos;Signed Manually&apos;
)</errorConditionFormula>
        <errorMessage>The related ICF must be signed or signed manually</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContractGen_Start_Date</fullName>
        <active>true</active>
        <description>Validation rule for contract generation: Start Date of SOW cannot be before Start Date of Framework Agreement</description>
        <errorConditionFormula>AND(
Can_Generate_Contract__c = &apos;true&apos;,
ISPICKVAL(Service_Type_HCPTS__c,&apos;Services&apos;),
Event_Speaker_Contract_HCPTS__r.Start_Date_vod__c &lt; Speaker_Contract_HCPTS__r.Start_Date_vod__c
)</errorConditionFormula>
        <errorMessage>Start Date of SOW cannot be before Start Date of Framework Agreement for a Service Contract</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContractGen_Subtype_cannot_be_blank</fullName>
        <active>true</active>
        <description>Validation rule that the Subtype cannot be blank when trying to generate a contract</description>
        <errorConditionFormula>AND(
Can_Generate_Contract__c = &apos;true&apos;,
ISPICKVAL(Service_Subtype_HCPTS__c,&apos;&apos;)
)</errorConditionFormula>
        <errorMessage>Contract Subtype cannot be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contract_Subtype_not_applicable</fullName>
        <active>false</active>
        <description>Contract Subtype should be applicable to selected Activity type</description>
        <errorConditionFormula>OR(

 
AND( Event_vod__r.RecordType.DeveloperName  = &apos;Advisory_Board&apos;, 
NOT(ISBLANK(TEXT(Service_Subtype_HCPTS__c))) ,
 TEXT(Service_Subtype_HCPTS__c)  &lt;&gt; &apos;Advisory Board&apos;
),
 AND( Event_vod__r.RecordType.DeveloperName  = &apos;Exhibits_Booths_and_Displays&apos;, 

 TEXT(Service_Subtype_HCPTS__c)  &lt;&gt; &apos;Meeting Space&apos;,
 TEXT(Service_Subtype_HCPTS__c)  &lt;&gt; &apos;Exhibit Space&apos;,
 TEXT(Service_Subtype_HCPTS__c)  &lt;&gt; &apos;Meeting &amp; Exhibit Space&apos;
,NOT(ISBLANK(TEXT(Service_Subtype_HCPTS__c))) 

)
,

 AND( Event_vod__r.RecordType.DeveloperName  = &apos;Internal_Speaking&apos;, 
 TEXT(Service_Subtype_HCPTS__c)  &lt;&gt; &apos;Internal Presentation&apos;
,NOT(ISBLANK(TEXT(Service_Subtype_HCPTS__c))) 
),
 AND( Event_vod__r.RecordType.DeveloperName  = &apos;Other_Consulting&apos;, 
 TEXT(Service_Subtype_HCPTS__c)  &lt;&gt; &apos;Content Preparation and/or Review&apos;,
 TEXT(Service_Subtype_HCPTS__c)  &lt;&gt; &apos;Recorded Presentation&apos;,
 TEXT(Service_Subtype_HCPTS__c)  &lt;&gt; &apos;Protocol Review&apos;,
 TEXT(Service_Subtype_HCPTS__c)  &lt;&gt; &apos;Presentation at a Congress&apos;
,NOT(ISBLANK(TEXT(Service_Subtype_HCPTS__c))) 
),
 AND( Event_vod__r.RecordType.DeveloperName  = &apos;Preceptorship&apos;, 
 TEXT(Service_Subtype_HCPTS__c)  &lt;&gt; &apos;Attendee for Preceptorship&apos;,
 TEXT(Service_Subtype_HCPTS__c)  &lt;&gt; &apos;Faculty for Preceptorship&apos;
,NOT(ISBLANK(TEXT(Service_Subtype_HCPTS__c))) 
),
 AND( Event_vod__r.RecordType.DeveloperName  = &apos;R_D_Committee&apos;, 
 TEXT(Service_Subtype_HCPTS__c)  &lt;&gt; &apos;Data Monitoring Committee&apos;,
 TEXT(Service_Subtype_HCPTS__c)  &lt;&gt; &apos;Publication Steering Committee&apos;,
 TEXT(Service_Subtype_HCPTS__c)  &lt;&gt; &apos;Study Steering Committee&apos;,
 TEXT(Service_Subtype_HCPTS__c)  &lt;&gt; &apos;FDA Mock Advisory Committee&apos;
,NOT(ISBLANK(TEXT(Service_Subtype_HCPTS__c))) 
),
 AND( Event_vod__r.RecordType.DeveloperName  = &apos;Speaker_Meeting&apos;, 
 TEXT(Service_Subtype_HCPTS__c)  &lt;&gt; &apos;Moderator&apos;,
 TEXT(Service_Subtype_HCPTS__c)  &lt;&gt; &apos;External Presentation (one-time)&apos;,
 TEXT(Service_Subtype_HCPTS__c)  &lt;&gt; &apos;External Presentation (Serial)&apos;
,NOT(ISBLANK(TEXT(Service_Subtype_HCPTS__c))) 
),

 
AND( Event_vod__r.RecordType.DeveloperName  = &apos;Speaker_Training&apos;, 
 TEXT(Service_Subtype_HCPTS__c)  &lt;&gt; &apos;Speaker Training&apos;
,NOT(ISBLANK(TEXT(Service_Subtype_HCPTS__c))) 
)
 
)</errorConditionFormula>
        <errorDisplayField>Service_Subtype_HCPTS__c</errorDisplayField>
        <errorMessage>Selected Contract Subtype is not applicable to Activity type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Framework_Agmnt_for_Sponsors_Events</fullName>
        <active>true</active>
        <description>Cannot have a Framework agreement for Sponsorship and Events Contract Type</description>
        <errorConditionFormula>AND(
NOT( ISBLANK(Speaker_Contract_HCPTS__c) ) ,

OR(
 ISPICKVAL( Service_Type_HCPTS__c , &apos;Sponsorship&apos;),
