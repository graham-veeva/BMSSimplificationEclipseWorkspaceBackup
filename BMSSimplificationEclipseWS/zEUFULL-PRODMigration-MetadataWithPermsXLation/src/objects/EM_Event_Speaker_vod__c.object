If(TEXT(Event_Speaker_Contract_HCPTS__r.Currency_HCPTS__c) = &quot;da-DK&quot;, &quot;DKK&quot;, &quot;&quot;) +
If(TEXT(Event_Speaker_Contract_HCPTS__r.Currency_HCPTS__c) = &quot;de-CH&quot;, &quot;CHF&quot;, &quot;&quot;) +
If(TEXT(Event_Speaker_Contract_HCPTS__r.Currency_HCPTS__c) = &quot;en-AU&quot;, &quot;AUD&quot;, &quot;&quot;) +
If(TEXT(Event_Speaker_Contract_HCPTS__r.Currency_HCPTS__c) = &quot;en-CA&quot;, &quot;CAD&quot;, &quot;&quot;) +
If(TEXT(Event_Speaker_Contract_HCPTS__r.Currency_HCPTS__c) = &quot;en-GB&quot;, &quot;GBP&quot;, &quot;&quot;) +
If(TEXT(Event_Speaker_Contract_HCPTS__r.Currency_HCPTS__c) = &quot;en-IN&quot;, &quot;INR&quot;, &quot;&quot;) +
If(TEXT(Event_Speaker_Contract_HCPTS__r.Currency_HCPTS__c) = &quot;en-NZ&quot;, &quot;NZD&quot;, &quot;&quot;) +
If(TEXT(Event_Speaker_Contract_HCPTS__r.Currency_HCPTS__c) = &quot;en-US&quot;, &quot;USD&quot;, &quot;&quot;) +
If(TEXT(Event_Speaker_Contract_HCPTS__r.Currency_HCPTS__c) = &quot;en-ZA&quot;, &quot;ZAR&quot;, &quot;&quot;) +
If(TEXT(Event_Speaker_Contract_HCPTS__r.Currency_HCPTS__c) = &quot;es-MX&quot;, &quot;MXN&quot;, &quot;&quot;) +
If(TEXT(Event_Speaker_Contract_HCPTS__r.Currency_HCPTS__c) = &quot;ja-JP&quot;, &quot;JPY&quot;, &quot;&quot;) +
If(TEXT(Event_Speaker_Contract_HCPTS__r.Currency_HCPTS__c) = &quot;nb-NO&quot;, &quot;NOK&quot;, &quot;&quot;) +
If(TEXT(Event_Speaker_Contract_HCPTS__r.Currency_HCPTS__c) = &quot;pl-PL&quot;, &quot;PLN&quot;, &quot;&quot;) +
If(TEXT(Event_Speaker_Contract_HCPTS__r.Currency_HCPTS__c) = &quot;pt-BR&quot;, &quot;BRL&quot;, &quot;&quot;) +
If(TEXT(Event_Speaker_Contract_HCPTS__r.Currency_HCPTS__c) = &quot;ru-RU&quot;, &quot;RUB&quot;, &quot;&quot;) +
If(TEXT(Event_Speaker_Contract_HCPTS__r.Currency_HCPTS__c) = &quot;zh-HK&quot;, &quot;HKD&quot;, &quot;&quot;)</formula>
