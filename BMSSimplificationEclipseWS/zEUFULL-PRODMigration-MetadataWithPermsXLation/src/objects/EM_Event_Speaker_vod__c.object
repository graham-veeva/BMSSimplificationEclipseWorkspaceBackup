)
)</errorConditionFormula>
        <errorDisplayField>Speaker_Contract_HCPTS__c</errorDisplayField>
        <errorMessage>Cannot associate a Framework agreement for Sponsorship and Events Contract Type</errorMessage>
    </validationRules>
