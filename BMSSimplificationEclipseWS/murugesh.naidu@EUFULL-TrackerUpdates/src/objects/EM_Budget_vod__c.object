    <validationRules>
        <fullName>Approver_cannot_be_Need_Owner</fullName>
        <active>true</active>
        <description>Approver cannot be Need Owner</description>
        <errorConditionFormula>OwnerId  =  Approving_User_HCPTS__c</errorConditionFormula>
        <errorMessage>Approver cannot be Need Owner</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_chose_both_WBS_Cost_Center</fullName>
        <active>true</active>
        <description>Validation rule to ensure either WBS Number or Cost Center is chosen</description>
        <errorConditionFormula>AND(NOT(ISBLANK(WBS_Number_HCPTS__c)),
    NOT(ISBLANK(Cost_Center_Code_HCPTS__c))
 )</errorConditionFormula>
        <errorMessage>Either WBS Code or Cost Center maybe specified but not both!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Company_Cd_Required_for_Available_Budget</fullName>
        <active>true</active>
        <description>To make a Budget &quot;Active&quot;  you must specify:
1. A valid WBS &amp; Company Code  
OR
2. A valid Cost Center and Company Code</description>
        <errorConditionFormula>IF(

AND (NOT(ISBLANK(Total_Budget_vod__c)),Total_Budget_vod__c&gt; 0),

AND (
      ISPICKVAL(Status_vod__c , &apos;Available_For_Use_vod&apos;) ,
      OR(
      ISBLANK( Company_Code_HCPTS__c),
      AND( 
         ISBLANK( WBS_Number_HCPTS__c ),
         ISBLANK( Cost_Center_Code_HCPTS__c)
      )
)), FALSE
)</errorConditionFormula>
        <errorDisplayField>Company_Code_HCPTS__c</errorDisplayField>
        <errorMessage>To make a Budget &quot;Active&quot;  you must specify:
1. A valid WBS &amp; Company Code  
OR
2. A valid Cost Center and Company Code</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_cannot_be_later_than_Start_Date</fullName>
        <active>true</active>
        <description>End Date cannot be later than start date</description>
        <errorConditionFormula>IF(
 AND(
     NOT(ISBLANK(Start_Date_vod__c)),
     NOT(ISBLANK(End_Date_vod__c))
     ) , 
    Start_Date_vod__c &gt; End_Date_vod__c, 
     FALSE
)</errorConditionFormula>
        <errorDisplayField>End_Date_vod__c</errorDisplayField>
        <errorMessage>End Date cannot be prior to Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_Have_Company_Code</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL(Status_vod__c,&apos;Available_For_Use_vod&apos;), 
ISBLANK(Company_Code_HCPTS__c) 
)</errorConditionFormula>
        <errorMessage>To make a Budget &quot;Active&quot; you must specify Company Code</errorMessage>
    </validationRules>
