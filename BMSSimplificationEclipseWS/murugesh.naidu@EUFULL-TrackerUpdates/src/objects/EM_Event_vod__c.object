    <validationRules>
        <fullName>Contract_Metadata_Submit_for_Approval</fullName>
        <active>false</active>
        <description>This rule will prevent a User from changing the status to In Business Approval if an Activity Speaker exists and a Activity Agreement exists yet there isn&apos;t at least one contract description and at least one fee/reimbursable</description>
        <errorConditionFormula>and(ISPICKVAL(Status_vod__c, &quot;In Business Approval&quot;) , Description_vod__c = &quot;DEMO&quot;)</errorConditionFormula>
        <errorMessage>You may not submit for Business Approval until at least one Contract Description and at least one Fee/Reimbursable exists for each Activity Speaker</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Estimate_Cannot_Exceed_Budget</fullName>
        <active>false</active>
        <description>When submitting an Activity for approval, the estimate cannot exceed the remaining budget</description>
        <errorConditionFormula>IF(ISBLANK(Inherited_Budget_Need_HCPTS__c),FALSE, 
IF(ISPICKVAL(Status_vod__c, &apos;In Business Approval&apos;),Estimated_Cost_vod__c &gt; Inherited_Budget_Need_HCPTS__r.Actual_Remaining_vod__c ,FALSE) 
)</errorConditionFormula>
        <errorMessage>The estimated amount cannot be greater than the remaining budget.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_Provider_Venue_Country</fullName>
        <active>true</active>
        <description>MNAIDU - 5/11/2017 - New VR to ensure Venue country is populated so that Integration does not cause error</description>
        <errorConditionFormula>AND( 
TEXT(Starcite_Request_Status__c) = &apos;Request Estimates&apos;,
 ISBLANK(Country_HCPTS__c)
)</errorConditionFormula>
        <errorMessage>Venue Country must be populated before moving Logistics Status to Request Estimates</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_fill_Congress_Info</fullName>
        <active>true</active>
        <description>MNAIDU - 5/11/2017 - UPDATED TO ONLY CHECK TO SEE IF CONGRESS NAMES ARE POPULATED WHEN BEFORE DURING OR AFTER IS ANSWERED AS YES WHEN REQUEST ESTIMATES IS STATUS FOR LOGISTICS</description>
        <errorConditionFormula>AND( 
TEXT(Starcite_Request_Status__c) = &apos;Request Estimates&apos;,
   OR(ISBLANK(TEXT(Before_during_or_after_a_Congress__c)),
      AND (
         ISPICKVAL(Before_during_or_after_a_Congress__c, &apos;Yes&apos;),
         ISBLANK(TEXT( Congress_Names_HCPTS__c ))
      )
    )
)</errorConditionFormula>
        <errorMessage>Before, during or after a Congress Must have a value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_have_a_Primary_Assigned_Need</fullName>
        <active>true</active>
        <description>Must define a Primary WBS/CC (Budget) on the Event prior to moving Logistics Request Status to Request Estimates.</description>
        <errorConditionFormula>AND 
(TEXT(Starcite_Request_Status__c) = &apos;Request Estimates&apos;, 
ISBLANK( Inherited_Budget_Need_HCPTS__c ) )</errorConditionFormula>
        <errorMessage>Must define a Primary WBS/CC (Budget) on the Event prior to moving Logistics Request Status to Request Estimates</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Fields_For_Business_Approval</fullName>
        <active>true</active>
        <description>Do not allow the activity to go into Business Approval Status unless all required fields are populated</description>
        <errorConditionFormula>AND( 
ISPICKVAL(Status_vod__c,&apos;In Business Approval&apos;), 
OR( 
AND( 
ISBLANK(TEXT(Activity_Format_HCPTS__c)), 
NOT(RecordType.Name = &apos;HCP_Sponsorship&apos;), 
NOT(RecordType.Name = &apos;Preceptorship&apos;), 
NOT(RecordType.Name = &apos;Exhibits_Booths_and_Displays&apos;) 

), 
AND( 
ISBLANK(Product_HCPTS__c), 
NOT(RecordType.Name = &apos;HCP_Sponsorship&apos;) 
), 

OR( 
ISBLANK(Location_vod__c), 
ISBLANK(Location_vod__c) 
) 
) 
)</errorConditionFormula>
        <errorMessage>The Activity cannot be submitted for approval until the following fields are completed: 
Activity Format,
Product/Therapeutic Area,
Venue Details</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_after_End_Date</fullName>
        <active>true</active>
        <description>Start Date/Time must be before the End Date/Time</description>
        <errorConditionFormula>Start_Time_vod__c &gt; End_Time_vod__c</errorConditionFormula>
        <errorMessage>Start Date/Time must be before the End Date/Time</errorMessage>
    </validationRules>
