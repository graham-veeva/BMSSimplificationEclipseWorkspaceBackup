MOD(DATEVALUE(Start_Time_vod__c) - DATE( 1900, 1, 7 ), 7) = 6
),

OR(
MOD(DATEVALUE(End_Time_vod__c) - DATE( 1900, 1, 7 ), 7) = 0,
MOD(DATEVALUE(End_Time_vod__c) - DATE( 1900, 1, 7 ), 7) = 6
),

DATEVALUE(End_Time_vod__c) - DATEVALUE(Start_Time_vod__c) &lt; 2

